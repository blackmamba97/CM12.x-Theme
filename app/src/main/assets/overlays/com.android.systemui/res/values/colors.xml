<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Qick settings color -->
    <color name="system_primary_color">@*common:color/p900</color>

    <!-- Notification bar header color -->
    <color name="system_secondary_color">@*common:color/p950</color>

    <!-- Brightness scrollbar color -->
    <color name="system_accent_color">@*common:color/s500</color>

    <!-- WIFI and BT qs Divider -->
    <color name="qs_tile_divider">@android:color/transparent</color>

    <color name="qs_detail_empty">@*common:color/qs</color>

    <color name="qs_detail_progress_track">#99E57373</color>

    <drawable name="heads_up_notification_bg_pressed">@*common:color/s500</drawable>

    <color name="qs_detail_transition">#99FF5722</color>

    <!-- Color for the equalizer tile -->
    <color name="visualizer_fill_color">@*common:color/s500</color>

    <!-- Navigation bar button ripple color -->
    <color name="navbutton_ripple_color">@*common:color/s500</color>

    <!-- SystemUI Tuner Icon Tint Color -->
    <color name="tuner_icon_tint">@android:color/transparent</color>

    <drawable name="ic_qs_bluetooth_detail_empty">@drawable/ic_qs_bluetooth_off</drawable>
    <drawable name="ic_qs_cast_detail_empty">@drawable/ic_qs_cast_off</drawable>
    <drawable name="ic_qs_wifi_detail_empty">@drawable/ic_qs_wifi_no_network</drawable>
    <drawable name="ic_qs_doze">@drawable/ic_qs_ambientdisplay_on</drawable>
    <drawable name="ic_qs_doze_off">@drawable/ic_qs_ambientdisplay_off</drawable>
    <drawable name="ic_qs_perf_profile">@drawable/ic_qs_perf_profile_vector</drawable>
    <drawable name="ic_qs_system_profiles">@drawable/ic_qs_profiles_on</drawable>
